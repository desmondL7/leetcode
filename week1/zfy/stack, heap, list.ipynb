   "source": [
    "class CQueue:\n",
    "\n",
    "    def __init__(self):\n",
    "        self.a,self.b=[],[]\n",
    "\n",
    "    def appendTail(self, value: int) -> None:\n",
    "        self.a.append(value)\n",
    "\n",
    "    def deleteHead(self) -> int:\n",
    "        if not self.b:\n",
    "            if not self.a:\n",
    "                return -1\n",
    "            else:\n",
    "                while self.a :\n",
    "                    self.b.append(self.a.pop())\n",
    "        return self.b.pop()"
