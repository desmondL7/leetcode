    "class MinStack:\n",
    "\n",
    "    def __init__(self):\n",
    "        \"\"\"\n",
    "        initialize your data structure here.\n",
    "        \"\"\"\n",
    "        self.a=[]\n",
    "        self.mini=[]\n",
    "\n",
    "\n",
    "    def push(self, x: int) -> None:\n",
    "        self.a.append(x)\n",
    "        if not self.mini or x<self.mini[-1]:\n",
    "            self.mini.append(x)\n",
    "        else:\n",
    "            self.mini.append(self.mini[-1])\n",
    "\n",
    "    def pop(self) -> None:\n",
    "        self.mini.pop()\n",
    "        return self.a.pop()\n",
    "\n",
    "    def top(self) -> int:\n",
    "        return self.a[-1]\n",
    "\n",
    "\n",
    "    def min(self) -> int:\n",
    "        return self.mini[-1]"
