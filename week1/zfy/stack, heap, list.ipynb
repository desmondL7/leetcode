    "class MaxQueue:\n",
    "\n",
    "    def __init__(self):\n",
    "        self.a,self.max1=[],[]\n",
    "\n",
    "    def max_value(self) -> int:\n",
    "        if self.a:\n",
    "            return self.max1[0]\n",
    "        else:\n",
    "            return -1\n",
    "    def push_back(self, value: int) -> None:\n",
    "        self.a.append(value)\n",
    "        while self.max1 and self.max1[-1]<value:\n",
    "            self.max1.pop()\n",
    "        self.max1.append(value)\n",
    "    def pop_front(self) -> int:\n",
    "        if self.a:\n",
    "            if self.a[0]==self.max1[0]:\n",
    "                self.max1.pop(0) \n",
    "            return  self.a.pop(0)\n",
    "        else:\n",
    "            return -1"
